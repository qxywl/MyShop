    <sql id="query">
	        <if test="productFlowerId != null and productFlowerId !=''">
					and t.product_flower_id=#{productFlowerId}
		    </if>
	        <if test="productId != null and productId !=''">
					and t.product_id=#{productId}
		    </if>
	        <if test="column3 != null and column3 !=''">
		        	and t.Column_3 like CONCAT(CONCAT('%', #{column3}), '%') 
		    </if>
	        <if test="column4 != null and column4 !=''">
		        	and t.Column_4 like CONCAT(CONCAT('%', #{column4}), '%') 
