          (#{productInfoAuthId},#{productName},#{productCode},#{catCode},#{catName},#{brandCode},#{brandName},#{supplierId},#{productDesc},#{productShelves},
           #{productId},#{productProducer},#{productWeight},#{productLong},
           #{productWidth},#{productHeight},#{productDensity},#{productAtrNumber},
           #{productShortName}, #{productPrefix},#{productSuffix},#{productKeyword},
