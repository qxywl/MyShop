<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ProductSizeDaoImpl">
	
	<resultMap type="ProductSizeModel" id="productSizeModel">
		   <result property="productSizeId" column="product_size_id" javaType="java.lang.Integer" />
		   <result property="sizeValue" column="size_value" javaType="java.lang.String" />
		   <result property="sortNum" column="sort_num" javaType="java.lang.Integer" />
	</resultMap>

 
	<select id="getAll" resultMap="productSizeModel">
		select 
	          product_size_id,
	    
	          size_value,
	    
	          sort_num
	          
	     from  t_product_size t
	</select>



</mapper>