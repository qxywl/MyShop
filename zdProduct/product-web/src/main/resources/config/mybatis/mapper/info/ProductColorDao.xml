<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ProductColorDaoImpl">
	
	<resultMap type="ProductColorModel" id="productColorModel">
		   <result property="productColorId" column="product_color_id" javaType="java.lang.Integer" />
		   <result property="colorValue" column="color_value" javaType="java.lang.String" />
		   <result property="sortNum" column="sort_num" javaType="java.lang.Integer" />
	</resultMap>

 
	<select id="getAll" resultMap="productColorModel">
		select 
	          product_color_id,
	    
	          color_value,
	    
	          sort_num
	          
	     from  t_product_color t
	</select>



</mapper>