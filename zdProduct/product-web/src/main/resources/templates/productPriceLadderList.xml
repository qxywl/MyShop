<?xml version="1.0" encoding="ISO-8859-1"?>
<workbook>
    <worksheet idx="0">        
        <loop startRow="1" endRow="1" items="list" var="ProductPriceLadderModel" varType="com.zhaidou.product.info.model.ProductPriceLadderModel">
            <section startRow="1" endRow="1">               
                <mapping row="1" col="1">ProductPriceLadderModel.productCode</mapping>
                <mapping row="1" col="2">ProductPriceLadderModel.priceLevel1</mapping>
                <mapping row="1" col="3">ProductPriceLadderModel.priceLevel2</mapping>   
                <mapping row="1" col="4">ProductPriceLadderModel.priceLevel3</mapping>
                <mapping row="1" col="5">ProductPriceLadderModel.priceLevel4</mapping>         				
            </section>
             <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"/>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>   
</workbook>